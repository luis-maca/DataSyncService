{
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Debug"
      }
    }
  },
  "AzureAd": {
    "Domain": "yourcompany.com",
    "Instance": "https://login.microsoftonline.com/",
    "TenantId": "xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx",
    "ClientId": "xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx",
    "Audience": "api://xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx",
    "CallbackPath": "/signin-oidc",
    "AllowWebApiToBeAuthorizedByACL": true
  },
  "AppSettings": {
  },
  "AzureServiceBusEnabled": true,
  "ConnectionStrings": {
    "CoreDatabase": "Server=tcp:yourserver;Database=yourdb;Authentication=Active Directory Default",
    "SecundaryDatabase": "Server=tcp:yourserver;Database=yourdb;Authentication=Active Directory Default",
    "Database": "Server=tcp:yourserver;Database=yourdb;Authentication=Active Directory Default"
  }
}